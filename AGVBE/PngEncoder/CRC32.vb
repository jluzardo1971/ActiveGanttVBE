    Friend Shared Function ComputeCrc32(oldCrc As UInteger, value As Byte) As UInteger
        Return CUInt(Crc32.CrcTable((oldCrc Xor value) And &HFF) Xor (oldCrc >> 8))
    End Function

    Public Property Value() As Long Implements IChecksum.Value
        Get
            Return CLng(crc)
        End Get
        Set(value As Long)
            crc = CUInt(value)
        End Set
    End Property

    Public Sub Reset() Implements IChecksum.Reset
        crc = 0
    End Sub

    Public Sub Update(value As Integer) Implements IChecksum.Update
        crc = crc Xor CrcSeed
        crc = CrcTable((crc Xor value) And &HFF) Xor (crc >> 8)
        crc = crc Xor CrcSeed
    End Sub

    Public Sub Update(buffer As Byte()) Implements IChecksum.Update
        If buffer Is Nothing Then
            Throw New ArgumentNullException("buffer")
        End If

        Update(buffer, 0, buffer.Length)
    End Sub

    Public Sub Update(buffer As Byte(), offset As Integer, count As Integer) Implements IChecksum.Update
        If buffer Is Nothing Then
            Throw New ArgumentNullException("buffer")
        End If

        If count < 0 Then

            Throw New ArgumentOutOfRangeException("count")
        End If

        If offset < 0 OrElse offset + count > buffer.Length Then
            Throw New ArgumentOutOfRangeException("offset")
        End If

        crc = crc Xor CrcSeed



        'While (--count >= 0)
        '{
        '	crc = CrcTable[(crc ^ buffer[offset++]) & 0xFF] ^ (crc >> 8);
        '}

        While PreD(count) >= 0
            crc = CrcTable((crc Xor buffer(PosI(offset))) And &HFF) Xor (crc >> 8)
        End While

        crc = crc Xor CrcSeed

        System.Diagnostics.Debug.WriteLine("crc: " & crc.ToString())

    End Sub
